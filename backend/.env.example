# Database Configuration
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/nanobanana

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Google Gemini API (Sprint 2)
GOOGLE_API_KEY=your_google_api_key_here

